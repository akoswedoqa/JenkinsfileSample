pipeline {
    agent any

    stages {
        stage('build') {
            steps {
                echo 'Hello World'
            }
        }
        stage('deploy') {
            steps {
                echo 'Hello Dev'
            }
        }
        stage('test') {
            steps {
                echo 'Hello Test'
            }
        }
        stage('release') {
            steps {
                echo 'Hello User'
            }
        }
    }
}
